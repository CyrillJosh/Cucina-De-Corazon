@using System.Text.Json
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Cucina_De_Corazon</title>
    <link href="https://cdn.jsdelivr.net/npm/fullcalendar@6.1.11/index.global.min.css" rel="stylesheet">


    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Cucina_De_Corazon.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/flatpikr.css"/>
</head>
<body class="d-flex flex-column vh-100 vw-100 overflow-hidden" style="background: #ECE0D8">
    <header style="background: #212120">
        <nav class="navbar navbar-expand-lg">
            <div class="container-fluid">
                <div class="w-100 d-flex flex-row justify-content-between px-5 align-items-center" style="font-size: 1.25rem; font-weight: bold;">
                    @{
                        var role = Context.Session.GetString("Role");
                        var sessionUser = Context.Session.GetString("User") ?? "";
                    }
                    @if (role == "Customer" || role == null)
                    {
                        <a class="text-decoration-none text-white" href="/"><img src="/images/logo.png" height="70" width="70" alt="Alternate Text" class="me-5"/> Home</a>
                    }
                    @if (role == "Admin")
                    {
                        <a class=" text-decoration-none text-white" href="/Records">Records</a>
                        <a class=" text-decoration-none text-white" href="/Menu">Menu</a>
                        <a class=" text-decoration-none text-white" href="/Feedback">Comments</a>
                        <a class=" text-decoration-none text-white" href="/Event/Calendar">Schedules</a>
                        <a class=" text-decoration-none text-white" href="/User">UserManagement</a>
                    }
                    else
                    {
                        <a class="text-decoration-none text-white"
                                data-bs-toggle="modal" data-bs-target="#Category">
                            Menu
                        </a>
                        <a class=" text-decoration-none text-white" href="#services">Our Services</a>
                        <a class=" text-decoration-none text-white" href="#booking">Book Your Event</a>
                        @if(!string.IsNullOrEmpty(sessionUser))
                        {
                            <a class=" text-decoration-none text-white" href="/Menu/MyOrders">My Orders</a>
                        }
                    }

                    <a href="/Cart" class=" text-decoration-none text-white bg-orange pe-2">
                        <svg width="43" height="42" viewBox="0 0 43 42" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                            <rect x="0.194275" y="0.0140228" width="41.972" height="41.972" fill="url(#pattern0_92_103)" />
                            <defs>
                                <pattern id="pattern0_92_103" patternContentUnits="objectBoundingBox" width="1" height="1">
                                    <use xlink:href="#image0_92_103" transform="scale(0.002)" />
                                </pattern>
                                <image id="image0_92_103" width="500" height="500" preserveAspectRatio="none" xlink:href="data:image/png;base64," />
                            </defs>
                        </svg>
                        CART
                    </a>
                    @if (string.IsNullOrEmpty(sessionUser))
                    {
                        <a class=" text-decoration-none text-white" asp-controller="User" asp-action="Login">Login</a>
                    }
                    else
                    {
                        <a class="text-decoration-none text-white" asp-controller="User" asp-action="Logout">Logout</a>
                    }
                </div>
            </div>
        </nav>
    </header>
    <div class="flex-grow-1 overflow-y-scroll">
            @RenderBody()
    </div>
    <div class="modal fade" id="Category" aria-labelledby="CategoryModal" aria-hidden="true" tabindex="-1">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content rounded-3">
                <div class="modal-header text-center d-flex flex-row align-items-center">
                    <h3 class="modal-title text-orange flex-grow-1">FOOD MENU</h3>
                    <button type="button" class="btn-close ms-0" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body d-flex flex-column gap-3 px-3" style="max-height: 300px; overflow-y: auto;">
                    @{
                        var categoriesJson = Context.Session.GetString("Categories");
                        List<Category> categories = null;
                        if (!string.IsNullOrEmpty(categoriesJson))
                        {
                            categories = JsonSerializer.Deserialize<List<Category>>(categoriesJson);
                        }
                    }
                    @foreach (var c in categories)
                    {
                        <a class="text-decoration-none text-center"
                           style="background: #f57c1f;
                                        color: #fff;
                                        font-weight: bold;
                                        padding: 10px 20px;
                                        border-radius: 6px;
                                        text-decoration: none;
                                        transition: all 0.3s ease;"
                           asp-controller="Menu"
                           asp-action="Products"
                           asp-route-category="@c.CategoryId" class="menu-link">
                            @c.CategoryName
                        </a>
                    }
                </div>
                <div class="modal-footer">
                    <a href="/Cart" class="btn btn-danger w-100" style=" padding: 10px 20px;">Go to Cart</a>
                </div>
            </div>
        </div>
    </div>

  @*   <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2025 - Cucina_De_Corazon - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer> *@
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/calendar.js"></script>
	<script src="~/js/flatpikr.js"></script>
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
